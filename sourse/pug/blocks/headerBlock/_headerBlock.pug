mixin headerBlock(data)
	// start headerBlock
	+b.headerBlock.section#headerBlock&attributes(attributes)
		.container
			+e.inner
				+b.section-title
					h1= data.title
				h3= data.text
				+e.listTitle= data.listTitle
				ul
					li= data.list1
					li= data.list2
			+e.btnWrap
				+e.reg(href="#")= data.reg
				+e.learn(href="#")= data.learn

						
	// end headerBlock